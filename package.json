{
  "name": "schema-doter",
  "version": "1.0.0",
  "main": "./schemaDefinitions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuef/node-cli-starter"
  },
  "author": {
    "name": "joshuef",
    "email": "joshuef@gmail.com",
    "url": "https://github.com/joshuef"
  },
  "scripts": {
    "start": "yarn rimraf ./schemaDefinitions && yarn ./lib/getter.js",
    "get": "yarn rimraf ./schemaDefinitions && yarn babel-node src/getter.js",
    "build": "rimraf ./lib && babel src -d lib",
    "compile-schemas": "babel ./schemaDefinitions/precompile.js --out-file ./schemaDefinitions/index.js",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn run lint --fix",
    "prepush": "yarn lint && yarn test",
    "test": "jest --maxWorkers=2",
    "bump": "standard-version",
    "postinstall": "yarn get && yarn compile-schemas"
  },
  "dependencies": {
    "bristol": "0.4.0",
    "commander": "2.17.1",
    "fs-extra": "7.0.0",
    "klaw": "3.0.0",
    "palin": "2.2.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.5.0",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-promise": "4.0.0",
    "jest": "23.5.0"
  }
}
